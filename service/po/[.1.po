#. type: Plain text
#: [.1:5
msgid "test - check file types and compare values"
msgstr "test - проверка типа файла и сравнение значений"

#. type: SH
#: [.1:5
#, no-wrap
msgid "SYNOPSIS"
msgstr "ОБЗОР"

#. type: Plain text
#: [.1:8
msgid "B<test> I<EXPRESSION>"
msgstr "B<test> I<ВЫРАЖЕНИЕ>"

#. type: Plain text
#: [.1:10
msgid "B<test>"
msgstr "B<test>"

#.  \& tells doclifter the brackets are literal (Bug#31803).
#. type: Plain text
#: [.1:15
msgid "B<[\\&> I<EXPRESSION> B<]\\&>"
msgstr "B<[\\&> I<ВЫРАЖЕНИЕ> B<]\\&>"

#. type: Plain text
#: [.1:17
msgid "B<[\\& ]\">"
msgstr "B<[\\& ]\">"

#. type: Plain text
#: [.1:20
msgid "B<[\\&> I<OPTION>"
msgstr "B<[\\&> I<ПАРАМЕТР>"

#. type: SH
#: [.1:20
#, no-wrap
msgid "DESCRIPTION"
msgstr "ОПИСАНИЕ"

#. type: Plain text
#: [.1:24
msgid "Exit with the status determined by EXPRESSION."
msgstr "Завершение со статусом, определённым ВЫРАЖЕНИЕМ."

#. type: TP
#: [.1:24
#, no-wrap
msgid "B<--help>"
msgstr "B<--help>"

#. type: Plain text
#: [.1:27
msgid "display this help and exit"
msgstr "вывод этого сообщения и выход"

#. type: TP
#: [.1:27
#, no-wrap
msgid "B<--version>"
msgstr "B<--version>"

#. type: Plain text
#: [.1:30
msgid "output version information and exit"
msgstr "вывод информации о версии и выход"

#. type: Plain text
#: [.1:33
msgid ""
"An omitted EXPRESSION defaults to false.  Otherwise, EXPRESSION is true or "
"false and sets exit status.  It is one of:"
msgstr ""
"Неуказанное ВЫРАЖЕНИЕ по умолчанию ложно. Иначе ВЫРАЖЕНИЕ верно или ложно и"
" задаёт статус завершения. Оно может иметь следующий вид:"

#. type: TP
#: [.1:33
#, no-wrap
msgid "( EXPRESSION )"
msgstr "( ВЫРАЖЕНИЕ )"

#. type: Plain text
#: [.1:36
msgid "EXPRESSION is true"
msgstr "ВЫРАЖЕНИЕ верно"

#. type: TP
#: [.1:36
#, no-wrap
msgid "! EXPRESSION"
msgstr "! ВЫРАЖЕНИЕ"

#. type: Plain text
#: [.1:39
msgid "EXPRESSION is false"
msgstr "ВЫРАЖЕНИЕ ложно"

#. type: TP
#: [.1:39
#, no-wrap
msgid "EXPRESSION1 B<-a> EXPRESSION2"
msgstr "ВЫРАЖЕНИЕ1 B<-a> ВЫРАЖЕНИЕ2"

#. type: Plain text
#: [.1:42
msgid "both EXPRESSION1 and EXPRESSION2 are true"
msgstr "ВЫРАЖЕНИЕ1 и ВЫРАЖЕНИЕ2 верны"

#. type: TP
#: [.1:42
#, no-wrap
msgid "EXPRESSION1 B<-o> EXPRESSION2"
msgstr "ВЫРАЖЕНИЕ1 B<-o> ВЫРАЖЕНИЕ2"

#. type: Plain text
#: [.1:45
msgid "either EXPRESSION1 or EXPRESSION2 is true"
msgstr "верно или ВЫРАЖЕНИЕ1, или ВЫРАЖЕНИЕ2"

#. type: TP
#: [.1:45
#, no-wrap
msgid "B<-n> STRING"
msgstr "B<-n> СТРОКА"

#. type: Plain text
#: [.1:48
msgid "the length of STRING is nonzero"
msgstr "длина СТРОКИ не равна нулю"

#. type: TP
#: [.1:48
#, no-wrap
msgid "STRING"
msgstr "STRING"

#. type: Plain text
#: [.1:51
msgid "equivalent to B<-n> STRING"
msgstr "равнозначно B<-n> СТРОКА"

#. type: TP
#: [.1:51
#, no-wrap
msgid "B<-z> STRING"
msgstr "B<-z> STRING"

#. type: Plain text
#: [.1:54
msgid "the length of STRING is zero"
msgstr "длина СТРОКИ равняется нулю"

#. type: TP
#: [.1:54
#, no-wrap
msgid "STRING1 = STRING2"
msgstr "СТРОКА1 = СТРОКА2"

#. type: Plain text
#: [.1:57
msgid "the strings are equal"
msgstr "строки равны"

#. type: TP
#: [.1:57
#, no-wrap
msgid "STRING1 != STRING2"
msgstr "СТРОКА1 != СТРОКА2"

#. type: Plain text
#: [.1:60
msgid "the strings are not equal"
msgstr "строки не равны"

#. type: TP
#: [.1:60
#, no-wrap
msgid "INTEGER1 B<-eq> INTEGER2"
msgstr "ЧИСЛО1 B<-eq> ЧИСЛО2"

#. type: Plain text
#: [.1:63
msgid "INTEGER1 is equal to INTEGER2"
msgstr "ЧИСЛО1 равно ЧИСЛУ2"

#. type: TP
#: [.1:63
#, no-wrap
msgid "INTEGER1 B<-ge> INTEGER2"
msgstr "ЧИСЛО1 B<-ge> ЧИСЛО2"

#. type: Plain text
#: [.1:66
msgid "INTEGER1 is greater than or equal to INTEGER2"
msgstr "ЧИСЛО1 больше или равно ЧИСЛУ2"

#. type: TP
#: [.1:66
#, no-wrap
msgid "INTEGER1 B<-gt> INTEGER2"
msgstr "ЧИСЛО1 B<-gt> ЧИСЛО2"

#. type: Plain text
#: [.1:69
msgid "INTEGER1 is greater than INTEGER2"
msgstr "ЧИСЛО1 больше ЧИСЛА2"

#. type: TP
#: [.1:69
#, no-wrap
msgid "INTEGER1 B<-le> INTEGER2"
msgstr "ЧИСЛО1 B<-le> ЧИСЛО2"

#. type: Plain text
#: [.1:72
msgid "INTEGER1 is less than or equal to INTEGER2"
msgstr "ЧИСЛО1 меньше или равно ЧИСЛУ2"

#. type: TP
#: [.1:72
#, no-wrap
msgid "INTEGER1 B<-lt> INTEGER2"
msgstr "ЧИСЛО1 B<-lt> ЧИСЛО2"

#. type: Plain text
#: [.1:75
msgid "INTEGER1 is less than INTEGER2"
msgstr "ЧИСЛО1 меньше ЧИСЛА2\t"

#. type: TP
#: [.1:75
#, no-wrap
msgid "INTEGER1 B<-ne> INTEGER2"
msgstr "ЧИСЛО1 B<-ne> ЧИСЛО2"

#. type: Plain text
#: [.1:78
msgid "INTEGER1 is not equal to INTEGER2"
msgstr "ЧИСЛО1 не равно ЧИСЛУ2"

#. type: TP
#: [.1:78
#, no-wrap
msgid "FILE1 B<-ef> FILE2"
msgstr "ФАЙЛ1 B<-ef> ФАЙЛ2"

#. type: Plain text
#: [.1:81
msgid "FILE1 and FILE2 have the same device and inode numbers"
msgstr "ФАЙЛ1 и ФАЙЛ1 имеют схожее устройство и индексный дескриптор (inode)"

#. type: TP
#: [.1:81
#, no-wrap
